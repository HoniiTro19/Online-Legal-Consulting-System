<?xml version="1.0" encoding="utf-8" ?>

<!--mybatis实现AccusationDao接口定义的功能-->

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.huidong.legalsys.dao.AccusationDao">

    <select id="getAccu" resultType="java.lang.String">
        select accu from accusationTable where id = #{id}
    </select>

</mapper>
